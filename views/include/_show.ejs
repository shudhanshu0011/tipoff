    <td><%= users[i].address %></td>
    <td><%= users[i].city %></td>
    <td><%= users[i].state %></td>
    <td><%= users[i].pincode %></td>
    <td><%= users[i].category %></td>
    <td><%= users[i].description %></td>
    <td>
        <!-- <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a> -->
        <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
            <span class="text-gradient" style="cursor: pointer;">Resolved</i></span>
        </a>
    </td>
</tr>
<% } %>